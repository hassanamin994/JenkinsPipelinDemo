pipeline {
  agent any
  
  stages {
    stage('Build') {
      steps {
        echo "Now building"
      }
    }
    stage('Testing') {
      steps {
        echo "Test"
      }
    }
    stage('Deploying') {
      steps {
        echo "Deploying"
      }
    }
    stage('Release') {
      steps {
        echo "Releasing"
      }
    }
  }

}
